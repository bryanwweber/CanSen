[run]
branch = True
source = cansen

[report]
exclude_lines =
    if self\.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    if typing.TYPE_CHECKING:
ignore_errors = True
omit =
    cansen/tests/*
    cansen/_version.py
    */tmp/*
    */private/*

[html]
directory = htmlcov
